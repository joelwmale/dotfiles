{
    "enabledPlugins": {
      "laravel-simplifier@laravel": true,
      "agent-browser@agent-browser": true
    },
    "alwaysThinkingEnabled": true,
    "permissions": {
      "allow": [
        "Bash(gh:*)",
        "Bash(curl:*)",
        "Bash(composer:*)",
        "Bash(vendor/bin/phpunit:*)",
        "Bash(vendor/bin/pest:*)",
        "Bash(git:*)",
        "Bash(php artisan:*)",
        "Bash(npm:*)",
        "Bash(npx:*)",
        "Bash(yarn:*)",
        "Bash(pnpm:*)",
        "Bash(php:*)",
        "Bash(docker:*)",
        "Bash(docker-compose:*)",
        "Bash(make:*)",
        "Bash(brew:*)",
        "Bash(sed:*)",
        "Bash(cat:*)",
        "Bash(ls:*)",
        "Bash(grep:*)",
        "Bash(find:*)",
        "Bash(echo:*)",
        "Bash(mkdir:*)",
        "Bash(cp:*)",
        "Bash(mv:*)",
        "Bash(touch:*)",
        "Bash(chmod:*)",
        "Bash(pwd:*)",
        "Bash(which:*)",
        "Bash(whoami:*)",
        "Bash(date:*)",
        "Bash(wc:*)",
        "Bash(sort:*)",
        "Bash(uniq:*)",
        "Bash(head:*)",
        "Bash(tail:*)",
        "Bash(diff:*)",
        "Bash(awk:*)",
        "Bash(cut:*)",
        "Bash(tr:*)",
        "Bash(open:*)",
        "WebSearch",
        "WebFetch"
      ],
      "defaultMode": "acceptEdits"
    }
  }